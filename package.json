{
  "name": "college-election-site",
  "version": "2.0.0",
  "description": "Modern College Election Management System - Secure, Fast, and User-Friendly",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --signal SIGTERM server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-install": "rm -rf node_modules package-lock.json && npm install",
    "setup": "node init.js",
    "backup": "node scripts/backup.js",
    "lint": "echo \"Linting not configured yet\"",
    "build": "echo \"Build process for production\""
  },
  "nodemonConfig": {
    "events": {
      "restart": "kill-port 3000 3001",
      "crash": "kill-port 3000 3001"
    },
    "delay": "1500"
  },
  "keywords": [
    "election",
    "voting",
    "college",
    "student",
    "democracy",
    "pwa",
    "responsive",
    "mongodb",
    "express",
    "nodejs"
  ],
  "author": "Pranay Gajbhiye",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pusparaj99op/College-Election-Site.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.17.3",
    "google-auth-library": "^8.9.0",
    "googleapis": "^123.0.0",
    "helmet": "^7.0.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "qrcode": "^1.5.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.1"
  }
}
